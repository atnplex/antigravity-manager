# Antigravity Manager - Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# Required: Your API key for accessing the proxy
API_KEY=change-me-to-secure-random-key

# Required: Password for the web UI admin panel
WEB_PASSWORD=change-me-to-secure-password

# Optional: Log level (debug, info, warn, error)
LOG_LEVEL=info

# Optional: Enable LAN access (0=localhost only, 1=bind to 0.0.0.0)
# Default: 0 (localhost-only for security)
# WARNING: Set to 1 ONLY if you also change docker-compose.yml ports to "8045:8045"
ALLOW_LAN=0
